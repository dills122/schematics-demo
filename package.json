{
  "name": "schematics-demo",
  "version": "0.0.0",
  "description": "A basic example of Angular Schematics extending ng-new for bootstrapping new apps",
  "type": "commonjs",
  "scripts": {
    "build": "node tools/clean-dist.cjs && tsc -p tsconfig.json && node tools/copy-assets.cjs",
    "test": "npm run build && jasmine dist/src/**/*_spec.js",
    "clean:workspaces": "rimraf ./.schematics",
    "generate:dry": "schematics ./dist/src/collection.json:schematics-demo --name dev-app --skip-install",
    "generate:test": "schematics ./dist/src/collection.json:schematics-demo --name dev-app --skip-install --debug=false",
    "scaffold": "npm run clean:workspaces && npm run build && npm run generate:test",
    "scaffold:dist": "npm run clean:workspaces && npm run build && schematics ./dist/src/collection.json:schematics-demo --name dist-app --skip-install --debug=false",
    "pack": "npm pack",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "schematics",
    "angular-schematics",
    "angular"
  ],
  "author": {
    "name": "Dylan Steele",
    "email": "dylansteele57@gmail.com"
  },
  "license": "MIT",
  "files": [
    "dist/",
    "README.md"
  ],
  "schematics": "./dist/src/collection.json",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@angular-devkit/core": "^20.3.6",
    "@angular-devkit/schematics": "^20.3.6",
    "@schematics/angular": "^20.3.6",
    "typescript": "~5.9.2"
  },
  "devDependencies": {
    "@angular-devkit/schematics-cli": "^20.3.6",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "jasmine": "^5.0.0",
    "rimraf": "^6.0.1"
  }
}
